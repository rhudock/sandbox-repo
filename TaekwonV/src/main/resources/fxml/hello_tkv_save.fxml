<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="350.0" prefWidth="650.0" spacing="0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lee.taekownv.helloTkvController">
    <children>
        <VBox prefHeight="286.0" prefWidth="420.0" spacing="10" maxHeight="Infinity" maxWidth="Infinity"
              minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
            <children>
                <HBox prefHeight="24.0" prefWidth="420.0"  maxHeight="Infinity" maxWidth="Infinity"
                      minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
                    <children>
                        <Label fx:id="msgLbl" prefWidth="150" text="FXML is cool!"/>
                        <Button fx:id="sayHelloBtn" onAction="#sayHello" text="Say Hello"/>
                    </children>
                </HBox>
                <Slider fx:id="timeSlider"/>
                <ProgressBar fx:id="timeProgressbar" prefHeight="18.0" prefWidth="613.0" progress="0.0"/>
                <Separator prefWidth="200.0"/>
                <HBox prefHeight="25.0" prefWidth="382.0" spacing="5">
                    <children>
                        <ChoiceBox fx:id="url2open" prefHeight="25.0" prefWidth="328.0"
                                   value="http://wpdev.chealwoo.com/">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="http://wpdev.chealwoo.com/"/>
                                    <String fx:value="5 minutes"/>
                                    <String fx:value="15 minutes"/>
                                </FXCollections>
                            </items>
                        </ChoiceBox>
                        <Button fx:id="startChrome" onAction="#startChrome" text="Start Chrome"/>
                    </children>
                </HBox>
                <Button fx:id="startEditPlus" onAction="#startEditPlus" text="Start EditPlus"/>
                <ImageView fx:id="tvImg" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#startYoutube"
                           pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="http://cfile229.uf.daum.net/image/150F2F434D8245B705737E"/>
                    </image>
                </ImageView>
            </children>
            <style>
                -fx-padding: 10;
                -fx-border-style: solid inside;
                -fx-border-width: 2;
                -fx-border-insets: 5;
                -fx-border-radius: 5;
                -fx-border-color: blue;
            </style>
        </VBox>
    </children>
</VBox>
